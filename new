            <script>
                
                var accordion = document.querySelector('.accordion');
                var accInfo = document.querySelector('.acc_info');
                var accordionImg = accordion.querySelector('img');

                console.log('accordion:', accordion);
                console.log('accInfo:', accInfo);
                console.log('accordionImg:', accordionImg);

                accordion.addEventListener('click', function () {
                    if (accInfo.classList.contains('active')) {
                        accInfo.classList.remove('active');
                        accInfo.style.maxHeight = '0';
                    } else {
                        accInfo.style.maxHeight = accInfo.scrollHeight + 'px';
                        accInfo.classList.add('active');
                    }
                    accordionImg.classList.toggle('rotate');
                });
            </script>















            <script>
                var details = document.querySelectorAll(".details_mobile");
                for (i = 0; i < details.length; i++) {
                    details[i].addEventListener("toggle", accordion);
                }
                function accordion(event) {
                    if (!event.target.open) return;
                    var details = event.target.parentNode.children;
                    for (i = 0; i < details.length; i++) {
                        if (details[i].tagName != "DETAILS" ||
                            !details[i].hasAttribute('open') ||
                            event.target == details[i]) {
                            continue;
                        }
                        details[i].removeAttribute("open");
                    }
                }
            </script>
            <script crossorigin="anonymous" src="js/jquery.slim.min.js"></script>
            <script crossorigin="anonymous" src="js/bootstrap.bundle.min.js"></script>
            <script src="js/jquery.min.js"></script>
            <script src="js/jquery.fancybox.min.js"></script>
            <script>
                function consoleBG() {
                    if ($(window).scrollTop() == 0) {
                        $('header').css('position', 'fixed');
                    } else {
                        $('header').css('position', 'fixed');
                    }
                }
                consoleBG();

                $(window).scroll(function () {
                    consoleBG();
                });
            </script>
            <script>
                const sh = document.documentElement.clientHeight + 100
                $('.mscrollbar').css('height', sh)
            </script>
            <script>
                $("#menu__toggle").on("click", function () {
                    if ($(this).is(":checked")) {
                        $("body").css('overflow', 'hidden')
                        $(".left_mobile_menu_btn").css('border-radius', '0px 0px 45px 0px')
                    } else {
                        $("body").css('overflow', 'initial')
                        $(".left_mobile_menu_btn").css('border-radius', '0px 0px 0px 45px')
                    }
                })</script>
            <script>
                $('.language-select').click(function () {
                    $(this).toggleClass('open');
                    $(".language-select li ").css('background', 'rgb(50,50,50)');
                })

                $('.language-select li').click(function () {
                    var setLang = $('.language-select').data('location'),
                        dataLangSelect = $(this).data('lang')
                    $('.language-select').data('location', dataLangSelect);
                    $('.language-select li').removeClass('active');
                    $(this).toggleClass('active');
                })
            </script>

